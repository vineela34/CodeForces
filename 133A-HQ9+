import java.util.*;
import java.lang.*;
import java.io.*;
public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str = sc.nextLine();
        for(int i=0;i<str.length();i++){
            char ch= str.charAt(i);
            if(ch == 'H' || ch == 'Q' || ch == '9'){
                System.out.println("YES");
                return;
            }
        }
        System.out.println("NO");      
        return;
    }
}
